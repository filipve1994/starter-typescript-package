{
  "name": "starter-typescript-package",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/starter-typescript-package.cjs",
  "module": "./dist/starter-typescript-package.mjs",
  "exports": {
    ".": {
      "require": "./dist/starter-typescript-package.cjs",
      "import": "./dist/starter-typescript-package.mjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf build/**/* && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "lint:scripts": "eslint . --ext .ts",
    "lint:scripts:v2": "eslint -c ./.eslintrc . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "pnpm run format:scripts && pnpm run format:styles",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "pnpm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.18.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^1.5.1",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.1"
  },
  "engines": {
    "node": ">=18.18.2"
  }
}
